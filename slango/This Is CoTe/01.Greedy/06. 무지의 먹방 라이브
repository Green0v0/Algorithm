# 3rd Try
def solution(food_times, k):
    N = len(food_times)
    # 무지가 먹어야 할 전체 음식량
    food_sum = sum(food_times)
    
    # 효율성을 위해 음식량이 에러발생시간 이전에 끝나는 경우는 -1 리턴하고 제외
    # 음식량이 그 시간 이후인 경우만 계산
    if food_sum > k:
        idx = 0
        
        while k:
            food_times[idx] -= 1
            idx = (idx + 1) % N
            while not food_times[idx]:
                idx = (idx + 1) % N
            k -= 1
            print(food_times, idx, k)
            
        return idx + 1
    else:
        return -1






    
